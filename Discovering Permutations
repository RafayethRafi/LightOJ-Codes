#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    int t,i;
    cin>>t;
    for(i=1;i<=t;i++)
    {
        cout<<"Case "<<i<<":"<<endl;
        int n,k,i,x=0;
        cin>>n>>k;
        string s;

        for(i=0;i<n;i++)
        {
            char c = i+'A';
            s+= c;
        }

        do
        {
            cout<<s<<endl;
            x++;
            if(x==k)break;
        }while(next_permutation(s.begin(),s.end()));

    }

    return 0;
}

